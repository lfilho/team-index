{
  "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true,
    },
    "globals": {
      "Highcharts": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "strict": 0,
      "quotes": 0,
      "no-unused-vars": 0,
      "camelcase": 0,
      "no-underscore-dangle": 0,
      "no-alert": 0,
      "no-process-exit": 0,
      "no-shadow": 0,
      "consistent-return": 0
    }
}
